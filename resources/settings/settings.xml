<settings>
	<setting propertyKey="@Properties.appVersion" title="@Strings.AppVersion">
        <settingConfig type="alphaNumeric" readonly="true" />
    </setting>
    
    <setting propertyKey="@Properties.largeFont" title="@Strings.useLargeFont">
    	<settingConfig type="boolean" />
    </setting>
    
    <setting propertyKey="@Properties.colorBackground" title="@Strings.colorBackground">
		<settingConfig type="list">
			<listEntry value="0x000000">@Strings.ColorBlack</listEntry>
			<listEntry value="0xAAAAAA">@Strings.ColorLtGray</listEntry>
			<listEntry value="0x555555">@Strings.ColorDkGray</listEntry>
			<listEntry value="0xFF0000">@Strings.ColorRed</listEntry>
			<listEntry value="0xFF5500">@Strings.ColorOrange</listEntry>
			<listEntry value="0xFFAA00">@Strings.ColorYellow</listEntry>
			<listEntry value="0x00FF00">@Strings.ColorGreen</listEntry>
			<listEntry value="0x00AA00">@Strings.ColorDkGreen</listEntry>
			<listEntry value="0x00AAFF">@Strings.ColorBlue</listEntry>
			<listEntry value="0x0000FF">@Strings.ColorDkBlue</listEntry>
			<listEntry value="0xFF00FF">@Strings.ColorPink</listEntry>
			<listEntry value="0xFFFFFF">@Strings.ColorWhite</listEntry>
		</settingConfig>
	</setting>
	
